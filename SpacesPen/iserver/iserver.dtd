<!ELEMENT flashMovie ((name, creator?, authorised?, unauthorised?, properties?), (template*), (content))>
<!ATTLIST flashMovie
	id ID #IMPLIED
	creator IDREF #IMPLIED
	properties IDREFS #IMPLIED
	authorised IDREFS #IMPLIED
	unauthorised IDREFS #IMPLIED
	templates IDREFS #IMPLIED
>

<!ELEMENT flashPropertySelector ((name, creator?, authorised?, unauthorised?, properties?), (layer?)?, (object), (propertyName, value, endValue))>
<!ATTLIST flashPropertySelector
	id ID #IMPLIED
	creator IDREF #IMPLIED
	properties IDREFS #IMPLIED
	authorised IDREFS #IMPLIED
	unauthorised IDREFS #IMPLIED
	layer IDREF #IMPLIED
	resource IDREF #REQUIRED
>

<!ELEMENT object (#PCDATA)>
<!ELEMENT propertyName (#PCDATA)>
<!ELEMENT endValue (#PCDATA)>

<!ELEMENT flashMethodSelector ((name, creator?, authorised?, unauthorised?, properties?), (layer?)?, (object), (methodName, arguments))>
<!ATTLIST flashMethodSelector
	id ID #IMPLIED
	creator IDREF #IMPLIED
	properties IDREFS #IMPLIED
	authorised IDREFS #IMPLIED
	unauthorised IDREFS #IMPLIED
	layer IDREF #IMPLIED
	resource IDREF #REQUIRED
>

<!ELEMENT methodName (#PCDATA)>

<!ELEMENT arguments (argument+)>
<!ELEMENT argument (key, value)>

<!ELEMENT selector (#PCDATA)>

<!ELEMENT name (#PCDATA)>

<!ELEMENT creator (individual)>
<!ATTLIST creator
	individual IDREF #IMPLIED
>

<!ELEMENT identifier (#PCDATA)>
<!ELEMENT uri (#PCDATA)>
<!ELEMENT timeout (#PCDATA)>
<!ELEMENT data (#PCDATA)>

<!ELEMENT authorised (individual* | group*)+>
<!ATTLIST authorised
	users IDREFS #IMPLIED
>
<!ELEMENT unauthorised (individual* | group*)*>
<!ATTLIST unauthorised
	users IDREFS #IMPLIED
>
<!ELEMENT properties (parameter+)>
<!ELEMENT sources (rectangle*)>
<!ELEMENT targets (rectangle*)+>
<!ELEMENT description (#PCDATA)>
<!ELEMENT preferences (parameter+)>
<!ELEMENT members (individual*, group*)>
<!ELEMENT login (#PCDATA)>
<!ELEMENT password (#PCDATA)>

<!ELEMENT id (#PCDATA)>
<!ELEMENT size (width, height)>
<!ATTLIST size
	id ID #IMPLIED
>
<!ELEMENT content (#PCDATA)>
<!ELEMENT pages (page+)>

<!ELEMENT number (#PCDATA)>

<!ELEMENT upperLeft (point)>

<!ELEMENT point (x, y)>

<!ELEMENT centre (point)>
<!ELEMENT radius (#PCDATA)>

<!ELEMENT width (#PCDATA)>
<!ELEMENT height (#PCDATA)>

<!ELEMENT shapes (rectangle* | polygon* | circle* | ellipse* | complexShape*)>

<!ELEMENT collection (#PCDATA)>

<!ELEMENT parameter (key, value)>
<!ATTLIST parameter
	id ID #IMPLIED
>

<!ELEMENT template ((name, creator?, authorised?, unauthorised?, properties?), (template*))>
<!ATTLIST template
	id ID #IMPLIED
	creator IDREF #IMPLIED
	properties IDREFS #IMPLIED
	authorised IDREFS #IMPLIED
	unauthorised IDREFS #IMPLIED
>

<!ELEMENT x (#PCDATA)>
<!ELEMENT y (#PCDATA)>

<!ELEMENT key (#PCDATA)>
<!ELEMENT value (#PCDATA)>

<!ELEMENT xhtmlDocument ((name, creator?, authorised?, unauthorised?, properties?), (uri))>
<!ATTLIST xhtmlDocument
	id ID #IMPLIED
	creator IDREF #IMPLIED
	properties IDREFS #IMPLIED
	authorised IDREFS #IMPLIED
	unauthorised IDREFS #IMPLIED
>

<!ELEMENT xhtmlSelector ((name, creator?, authorised?, unauthorised?, properties?), (layer?)?, (xpointer))>
<!ATTLIST xhtmlSelector
	id ID #IMPLIED
	creator IDREF #IMPLIED
	properties IDREFS #IMPLIED
	authorised IDREFS #IMPLIED
	unauthorised IDREFS #IMPLIED
	layer IDREF #IMPLIED
	resource IDREF #REQUIRED
>

<!ELEMENT xpointer (#PCDATA)>

