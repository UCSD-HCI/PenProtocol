	unauthorised IDREFS #IMPLIED
	layer IDREF #IMPLIED
	resource IDREF #REQUIRED
	shapes IDREFS #IMPLIED
>
<!ELEMENT textComp ((name, creator?, authorised?, unauthorised?, properties?), (template*), (content))>
<!ATTLIST textComp
	id ID #IMPLIED
	creator IDREF #IMPLIED
	properties IDREFS #IMPLIED
	authorised IDREFS #IMPLIED
	unauthorised IDREFS #IMPLIED
	templates IDREFS #IMPLIED
>
<!ELEMENT medium ((name, creator?, authorised?, unauthorised?, properties?), (template*), (description, content, collection?))>
<!ATTLIST medium
	id ID #IMPLIED
	creator IDREF #IMPLIED
	properties IDREFS #IMPLIED
	authorised IDREFS #IMPLIED
	unauthorised IDREFS #IMPLIED
	templates IDREFS #IMPLIED
>
<!ELEMENT omswe ((name, creator?, authorised?, unauthorised?, properties?), (template*), (description, content, collection?))>
<!ATTLIST omswe
	id ID #IMPLIED
	creator IDREF #IMPLIED
	properties IDREFS #IMPLIED
	authorised IDREFS #IMPLIED
	unauthorised IDREFS #IMPLIED
	templates IDREFS #IMPLIED
>
<!ELEMENT activeComponent ((name, creator?, authorised?, unauthorised?, properties?), (template*), (identifier, timeout?))>
<!ATTLIST activeComponent
	id ID #IMPLIED
	creator IDREF #IMPLIED
	properties IDREFS #IMPLIED
	authorised IDREFS #IMPLIED
	unauthorised IDREFS #IMPLIED
	templates IDREFS #IMPLIED
>
<!ELEMENT rfidSpace ((name, creator?, authorised?, unauthorised?, properties?), (template*))>
<!ATTLIST rfidSpace
	id ID #IMPLIED
	creator IDREF #IMPLIED
	properties IDREFS #IMPLIED
	authorised IDREFS #IMPLIED
	unauthorised IDREFS #IMPLIED
	templates IDREFS #IMPLIED
>
<!ELEMENT rfidTag ((name, creator?, authorised?, unauthorised?, properties?), (layer?)?, (id))>
<!ATTLIST rfidTag
	id ID #IMPLIED
	creator IDREF #IMPLIED
