<?xml version="1.0" encoding="UTF-8"?>
<!--iServer XML import/export format-->
<!ELEMENT iserver (link* | individual* | group* | layer* | document* | page* | rectangle* | polygon* | circle* | ellipse* | complexShape* | textComp* | medium* | omswe* | activeComponent* | rfidSpace* | rfidTag* | parameter* | xhtmlDocument* | xhtmlSelector* | template* | flashMovie* | flashPropertySelector* | flashMethodSelector*)+>

<!ELEMENT link ((name, creator?, authorised?, unauthorised?, properties?), (sources?, targets?))>
<!ATTLIST link
	id ID #IMPLIED
	creator IDREF #IMPLIED
	properties IDREFS #IMPLIED
	authorised IDREFS #IMPLIED
	unauthorised IDREFS #IMPLIED
	templates IDREFS #IMPLIED
	sources IDREFS #IMPLIED
	targets IDREFS #IMPLIED
>
<!ELEMENT individual ((name, description, preferences?, members?), (login, password))>
<!ATTLIST individual
	id ID #IMPLIED
	preferences IDREFS #IMPLIED
>
<!ELEMENT group (name, description, preferences?, members?)>
<!ATTLIST group
	id ID #IMPLIED
	preferences IDREFS #IMPLIED
	members IDREFS #IMPLIED
>
<!ELEMENT layer (name)>
<!ATTLIST layer
	id ID #IMPLIED
>
<!ELEMENT document ((name, creator?, authorised?, unauthorised?, properties?), (template*), (id, size?, content, pages?))>
<!ATTLIST document
	id ID #IMPLIED
	creator IDREF #IMPLIED
	properties IDREFS #IMPLIED
	authorised IDREFS #IMPLIED
	unauthorised IDREFS #IMPLIED
	templates IDREFS #IMPLIED
	size IDREF #IMPLIED
	pages IDREFS #IMPLIED
>
<!ELEMENT page ((name, creator?, authorised?, unauthorised?, properties?), (template*), (size?, number, document?))>
<!ATTLIST page
	id ID #IMPLIED
	creator IDREF #IMPLIED
	properties IDREFS #IMPLIED
	authorised IDREFS #IMPLIED
	unauthorised IDREFS #IMPLIED
	templates IDREFS #IMPLIED
	document IDREF #REQUIRED
	size IDREF #IMPLIED
>
<!ELEMENT rectangle ((name, creator?, authorised?, unauthorised?, properties?), (layer?)?, (upperLeft, size))>
<!ATTLIST rectangle
	id ID #IMPLIED
	creator IDREF #IMPLIED
